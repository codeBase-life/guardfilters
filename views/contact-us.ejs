    <section class="section-3 contact-form" style="background-color:#FCFCFC;">
        <div class="container mt-5 mt-md-0">
            <h3>Send us a message</h3>
        </div>
        <div class="container">
            <input type="text" class="form-control my-3" placeholder="Name">
            <input type="email" class="form-control my-3" placeholder="Email">
            <input type="text" class="form-control my-3" placeholder="Subject">
            <textarea class="form-control my-3" placeholder="Message" rows="5"></textarea>
            <div class="d-grid ">

                <button class="btn btn-danger" type="button">Contact us</button>
            </div>
        </div>
    </section>



    <section class="section-4">
